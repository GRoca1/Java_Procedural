import java.util.*;

public class Nb_de_Jeunes
{
	public static void main ( String args [])
	{
		int a , c = 0;
		for ( int i = 0; i < 20; i ++) {
			System.out.println ("Veuillez saisir l'âge de la personne n° :" + (i + 1)); 
			a = Lire . i () ;
			if ( a >= 20) c = c +1;
		}
		System.out.println ("Il y a : "+ c +" plus de 20 ans et "+(20 - c) +" moins de vinght ans ") ;
	}
}